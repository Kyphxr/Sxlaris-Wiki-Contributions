    #new-ones {
        display: flex;
        flex-direction: column-reverse;
    }
    #new-ones li p {
        margin: 0.625em auto;
    }
    #new-ones > li:not(:nth-last-of-type(-n+3)) {
        display: none;
    }
    </style>
